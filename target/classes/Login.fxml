<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.LoginController">
   <children>
      <AnchorPane layoutY="-1.0" prefHeight="500.0" prefWidth="350.0" style="-fx-background-color: #556B2F;">
         <children>
            <Text fill="WHITE" layoutX="54.0" layoutY="266.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Festival World">
               <font>
                  <Font size="39.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
      <AnchorPane layoutX="350.0" prefHeight="500.0" prefWidth="350.0">
         <children>
            <TextField fx:id="userField" alignment="CENTER" layoutX="94.0" layoutY="145.0" promptText="User Name" style="-fx-background-color: transparent; -fx-border-color: #556B2F; -fx-border-width: 0px 0px 2px 0px;" />
            <PasswordField fx:id="passwordField" alignment="CENTER" layoutX="94.0" layoutY="229.0" promptText="Password" style="-fx-background-color: transparent; -fx-border-color: #556B2F; -fx-border-width: 0px 0px 2px 0px;" />
            <Button fx:id="loginButton" layoutX="113.0" layoutY="325.0" mnemonicParsing="false" onAction="#LoginButtonAction" prefHeight="44.0" prefWidth="125.0" style="-fx-background-color: #556B2F;" text="Login" textFill="#fffefe" />
            <Text fill="DARKOLIVEGREEN" layoutX="113.0" layoutY="112.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Festival Login">
               <font>
                  <Font size="21.0" />
               </font>
            </Text>
            <Button layoutX="307.0" mnemonicParsing="false" onAction="#exit" prefHeight="41.0" prefWidth="43.0" style="-fx-background-color: transparent;" text="x" />
            <Button fx:id="registerButton" layoutX="113.0" layoutY="396.0" mnemonicParsing="false" onAction="#RegisterButtonAction" prefHeight="44.0" prefWidth="125.0" style="-fx-background-color: #556B2F;" text="Register" textFill="WHITE" />
            <Label fx:id="loginMessageLabel" alignment="CENTER" layoutX="104.0" layoutY="277.0" prefHeight="21.0" prefWidth="144.0" textFill="DARKOLIVEGREEN" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
